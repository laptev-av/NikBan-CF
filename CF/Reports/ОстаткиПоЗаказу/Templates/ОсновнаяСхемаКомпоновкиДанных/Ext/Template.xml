		<field xsi:type="DataSetFieldField">
			<dataPath>Выполнено</dataPath>
			<field>Выполнено</field>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ ПЕРВЫЕ 1
	ЗаказПокупателя.Магазин,
	ЗаказПокупателя.Склад
ПОМЕСТИТЬ ТаблицаМагазинСклад
ИЗ
	Документ.ЗаказПокупателя КАК ЗаказПокупателя
ГДЕ
	ЗаказПокупателя.Ссылка = &amp;ЗаказПокупателя
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ЗаказПокупателяТовары.НомерСтроки,
	ЗаказПокупателяТовары.Номенклатура,
	ЗаказПокупателяТовары.Характеристика,
	ЗаказПокупателяТовары.КодСтроки,
	ЗаказПокупателяТовары.Количество,
	ЗаказПокупателяТовары.Отменено,
	ЗаказПокупателяТовары.ПричинаОтмены,
	ЗаказПокупателяТовары.Резервировать,
	ЗаказПокупателяТовары.ПричинаОтмены.Наименование
ПОМЕСТИТЬ ТаблицаДокумента
ИЗ
	Документ.ЗаказПокупателя.Товары КАК ЗаказПокупателяТовары
ГДЕ
	ЗаказПокупателяТовары.Ссылка = &amp;ЗаказПокупателя
	И ЗаказПокупателяТовары.Ссылка.Проведен
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ТоварыНаСкладах.НомерСтроки,
	ТоварыНаСкладах.Номенклатура,
	ТоварыНаСкладах.Характеристика,
	ТоварыНаСкладах.КодСтроки,
	СУММА(ТоварыНаСкладах.Резерв) КАК Резерв
ПОМЕСТИТЬ ТаблицаРезерва
ИЗ
	РегистрНакопления.ТоварыНаСкладах КАК ТоварыНаСкладах
ГДЕ
	ТоварыНаСкладах.Регистратор = &amp;ЗаказПокупателя

СГРУППИРОВАТЬ ПО
	ТоварыНаСкладах.Номенклатура,
	ТоварыНаСкладах.Характеристика,
	ТоварыНаСкладах.НомерСтроки,
	ТоварыНаСкладах.КодСтроки
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ЗаказыПокупателейОстатки.Номенклатура,
	ЗаказыПокупателейОстатки.Характеристика,
	ЗаказыПокупателейОстатки.КодСтроки,
	СУММА(ЗаказыПокупателейОстатки.ЗаказаноОстаток) КАК ЗаказаноОстаток
ПОМЕСТИТЬ ТаблицаОстатковЗаказа
ИЗ
	РегистрНакопления.ЗаказыПокупателей.Остатки(, Заказ = &amp;ЗаказПокупателя) КАК ЗаказыПокупателейОстатки

СГРУППИРОВАТЬ ПО
	ЗаказыПокупателейОстатки.Номенклатура,
	ЗаказыПокупателейОстатки.Характеристика,
	ЗаказыПокупателейОстатки.КодСтроки
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ТаблицаРегистраПоСкладам.Номенклатура,
	ТаблицаРегистраПоСкладам.Характеристика,
	СУММА(ТаблицаРегистраПоСкладам.СвободныйОстатокСклад) КАК СвободныйОстатокСклад,
	СУММА(ТаблицаРегистраПоСкладам.СвободныйОстатокМагазин) КАК СвободныйОстатокМагазин,
	СУММА(ТаблицаРегистраПоСкладам.СвободныйОстатокСеть) КАК СвободныйОстатокСеть
ПОМЕСТИТЬ ТаблицаОстатковТоваров
ИЗ
	(ВЫБРАТЬ
		ТоварыНаСкладахОстатки.Склад КАК Склад,
		ТоварыНаСкладахОстатки.Склад.Магазин КАК Магазин,
		ТоварыНаСкладахОстатки.Номенклатура КАК Номенклатура,
		ТоварыНаСкладахОстатки.Характеристика КАК Характеристика,
		ВЫБОР
			КОГДА ТоварыНаСкладахОстатки.Склад В
					(ВЫБРАТЬ
						ТаблицаМагазинСклад.Склад
					ИЗ
						ТаблицаМагазинСклад КАК ТаблицаМагазинСклад)
				ТОГДА ТоварыНаСкладахОстатки.КоличествоОстаток - ТоварыНаСкладахОстатки.РезервОстаток
			ИНАЧЕ 0
		КОНЕЦ КАК СвободныйОстатокСклад,
		ВЫБОР
			КОГДА ТоварыНаСкладахОстатки.Склад.Магазин В
						(ВЫБРАТЬ
							ТаблицаМагазинСклад.Магазин
						ИЗ
							ТаблицаМагазинСклад КАК ТаблицаМагазинСклад)
					И НЕ ТоварыНаСкладахОстатки.Склад В
							(ВЫБРАТЬ
								ТаблицаМагазинСклад.Склад
							ИЗ
								ТаблицаМагазинСклад КАК ТаблицаМагазинСклад)
				ТОГДА ТоварыНаСкладахОстатки.КоличествоОстаток - ТоварыНаСкладахОстатки.РезервОстаток
			ИНАЧЕ 0
		КОНЕЦ КАК СвободныйОстатокМагазин,
		ВЫБОР
			КОГДА НЕ ТоварыНаСкладахОстатки.Склад.Магазин В
						(ВЫБРАТЬ
							ТаблицаМагазинСклад.Магазин
						ИЗ
							ТаблицаМагазинСклад КАК ТаблицаМагазинСклад)
				ТОГДА ТоварыНаСкладахОстатки.КоличествоОстаток - ТоварыНаСкладахОстатки.РезервОстаток
			ИНАЧЕ 0
		КОНЕЦ КАК СвободныйОстатокСеть
	ИЗ
		РегистрНакопления.ТоварыНаСкладах.Остатки(
				,
				Номенклатура В
					(ВЫБРАТЬ
						ТаблицаДокумента.Номенклатура
					ИЗ
						ТаблицаДокумента КАК ТаблицаДокумента)) КАК ТоварыНаСкладахОстатки
	ГДЕ
		ТоварыНаСкладахОстатки.КоличествоОстаток - ТоварыНаСкладахОстатки.РезервОстаток &gt; 0) КАК ТаблицаРегистраПоСкладам

СГРУППИРОВАТЬ ПО
	ТаблицаРегистраПоСкладам.Номенклатура,
	ТаблицаРегистраПоСкладам.Характеристика
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ЗаказыПокупателей.КодСтроки,
	ЗаказыПокупателей.Номенклатура,
	ЗаказыПокупателей.Характеристика,
	СУММА(ЗаказыПокупателей.Заказано) КАК Продано
ПОМЕСТИТЬ ТаблицаПродажи
ИЗ
	РегистрНакопления.ЗаказыПокупателей КАК ЗаказыПокупателей
ГДЕ
	ЗаказыПокупателей.Заказ = &amp;ЗаказПокупателя
	И ЗаказыПокупателей.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Расход)

СГРУППИРОВАТЬ ПО
	ЗаказыПокупателей.Номенклатура,
	ЗаказыПокупателей.Характеристика,
	ЗаказыПокупателей.КодСтроки
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ТаблицаДокумента.НомерСтроки,
	ТаблицаДокумента.Номенклатура,
	ТаблицаДокумента.Характеристика,
	ТаблицаДокумента.КодСтроки,
	ТаблицаДокумента.Количество КАК Заказано,
	ТаблицаДокумента.Отменено,
	ТаблицаДокумента.ПричинаОтмены,
	ТаблицаДокумента.Резервировать,
	ЕСТЬNULL(ТаблицаРезерва.Резерв, 0) КАК Резерв,
	ЕСТЬNULL(ТаблицаОстатковЗаказа.ЗаказаноОстаток, 0) КАК ЗаказаноОстаток,
	ЕСТЬNULL(ТаблицаПродажи.Продано, 0) КАК Продано,
	ЕСТЬNULL(ТаблицаОстатковТоваров.СвободныйОстатокСклад, 0) КАК СвободныйОстатокСклад,
	ЕСТЬNULL(ТаблицаОстатковТоваров.СвободныйОстатокМагазин, 0) КАК СвободныйОстатокМагазин,
	ЕСТЬNULL(ТаблицаОстатковТоваров.СвободныйОстатокСеть, 0) КАК СвободныйОстатокСеть,
	ВЫБОР
		КОГДА ТаблицаДокумента.Отменено
			ТОГДА "Отменено" + ВЫБОР
					КОГДА ТаблицаДокумента.ПричинаОтмены = ЗНАЧЕНИЕ(Справочник.ПричиныОтменыЗаказовПокупателей.ПустаяСсылка)
						ТОГДА ""
					ИНАЧЕ " (" + ТаблицаДокумента.ПричинаОтменыНаименование + ")"
				КОНЕЦ
		ИНАЧЕ ""
	КОНЕЦ КАК ОтмененоСообщение,
	ВЫБОР
		КОГДА НЕ ТаблицаДокумента.Количество = 0
				И ЕСТЬNULL(ТаблицаПродажи.Продано, 0) &gt; 0
				И ЕСТЬNULL(ТаблицаОстатковЗаказа.ЗаказаноОстаток, 0) = 0
			ТОГДА ИСТИНА
		ИНАЧЕ ЛОЖЬ
	КОНЕЦ КАК Выполнено
ИЗ
	ТаблицаДокумента КАК ТаблицаДокумента
		ЛЕВОЕ СОЕДИНЕНИЕ ТаблицаРезерва КАК ТаблицаРезерва
		ПО ТаблицаДокумента.Номенклатура = ТаблицаРезерва.Номенклатура
			И ТаблицаДокумента.Характеристика = ТаблицаРезерва.Характеристика
			И ТаблицаДокумента.КодСтроки = ТаблицаРезерва.КодСтроки
		ЛЕВОЕ СОЕДИНЕНИЕ ТаблицаОстатковЗаказа КАК ТаблицаОстатковЗаказа
		ПО ТаблицаДокумента.КодСтроки = ТаблицаОстатковЗаказа.КодСтроки
		ЛЕВОЕ СОЕДИНЕНИЕ ТаблицаОстатковТоваров КАК ТаблицаОстатковТоваров
		ПО ТаблицаДокумента.Номенклатура = ТаблицаОстатковТоваров.Номенклатура
			И ТаблицаДокумента.Характеристика = ТаблицаОстатковТоваров.Характеристика
		ЛЕВОЕ СОЕДИНЕНИЕ ТаблицаПродажи КАК ТаблицаПродажи
		ПО ТаблицаДокумента.КодСтроки = ТаблицаПродажи.КодСтроки</query>
