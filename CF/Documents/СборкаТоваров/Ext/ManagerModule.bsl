	Запрос.Текст = ТекстЗапроса;
	
	Результат = Запрос.ВыполнитьПакет();
	
	ТаблицаСебестоимостьНоменклатуры = Результат[7].Выгрузить();
	
	Если НЕ Реквизиты.СборкаКомплектующих Тогда
		ТаблицаСебестоимостьКомплекта = Результат[6].Выгрузить();
